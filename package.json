{
  "name": "kenya-locations",
  "version": "0.2.0",
  "description": "A comprehensive and intuitive TypeScript package for working with Kenyan administrative divisions (counties, sub-counties, constituencies, and wards)",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "David Amunga <https://davidamunga.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidAmunga/kenya-locations.git"
  },
  "bugs": {
    "url": "https://github.com/DavidAmunga/kenya-locations/issues"
  },
  "homepage": "https://kenya-locations.web.app",
  "files": [
    "dist"
  ],
  "keywords": [
    "kenya",
    "counties",
    "constituencies",
    "wards",
    "administrative",
    "divisions",
    "locations",
    "geospatial"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig.json && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "validate": "node scripts/validate-data.js",
    "lint": "eslint . --no-warn-ignored",
    "lint:fix": "eslint . --fix --no-warn-ignored",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "prepare-release": "bash scripts/prepare-release.sh",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "pnpm build && changeset publish"
  },
  "dependencies": {
    "fuse.js": "^7.1.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@eslint/js": "^9.27.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^4.0.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix --max-warnings 50 --no-warn-ignored",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "lib/data/*.ts": [
      "node scripts/validate-staged.js"
    ]
  }
}
